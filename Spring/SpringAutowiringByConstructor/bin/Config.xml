<?xml version="1.0" encoding="UTF-8"?>  
	
	<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 
  	<bean id="stud1" class="Student" autowire="constructor">
 		<constructor-arg value="Bharathi"></constructor-arg>
 		<constructor-arg value="9.88" type="double"></constructor-arg>
 	
  	</bean>
  	
  	<bean id="phoneNumber" class="PhoneNumber">
  		<property name="code" value="+91"></property>
  		<property name="number" value="9597307529"></property>
  	</bean>
  	
  	<bean id="arrears" class="Arrears">
	  	<property name="arrears">
		  	<list>
				<value>M1</value>
				<value>M2</value>
				<value>Unix</value>  	
		  	</list>
	  	</property>
  	</bean>
  </beans>
  
  